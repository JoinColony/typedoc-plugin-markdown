{
  "name": "typedoc-plugin-markdown",
  "version": "3.12.0",
  "description": "A plugin for TypeDoc that enables TypeScript API documentation to be generated in Markdown.",
  "main": "dist",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn run lint && yarn run build && yarn run test",
    "build": "rm -rf dist && tsc",
    "pretestx": "yarn run demo:md",
    "test": "jest ---colors -t \"Member Partial:\" --updateSnapshot",
    "build-and-test": "yarn run build && yarn run test",
    "docs": "yarn run build && yarn run docs:md && yarn run docs:html",
    "docs:md": "typedoc --options ./docs/options.json --plugin ./dist  --plugin ./docs/plugin --plugin typedoc-plugin-mdn-links --out ./docs/out/md",
    "docs:html": "typedoc --options ./docs/options.json --plugin ./docs/plugin --out ./docs/out/html",
    "demos": "yarn run build && yarn run demo:md && yarn run demo:html",
    "demo:md": "typedoc --plugin typedoc-plugin-markdown --namedAnchors  --plugin typedoc-plugin-mdn-links --options ./demo.options.json --out ./demo/md",
    "demo:html": "typedoc --options ./demo.options.json --plugin typedoc-plugin-mdn-links --out ./demo/html"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-markdown/tree/master/packages/typedoc-plugin-markdown",
  "keywords": [
    "markdown",
    "typescript",
    "documentation",
    "api",
    "typedocplugin",
    "typedoc"
  ],
  "peerDependencies": {
    "typedoc": ">=0.22.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "gitHead": "9f53c19295c5db082e0143eb9e833994c8d5bcec"
}
