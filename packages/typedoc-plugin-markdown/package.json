{
  "name": "@colony/typedoc-plugin-markdown",
  "version": "3.14.0",
  "description": "A fork of typedoc-plugin-markdown with a different layout better suited for GitBook",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn run lint && yarn run build && yarn run test",
    "build": "rm -rf dist && tsc && copyfiles --up 1 ./src/**/*.hbs ./dist/",
    "pretest": "yarn run demo:md && markdownlint ./demo/md",
    "test": "jest --colors",
    "build-and-test": "yarn run build && yarn run test",
    "fixtures": "node ./tasks/fixtures.js",
    "demos": "yarn run build && yarn run demo:md && yarn run demo:html",
    "demo:md": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-plugin-mdn-links --options ./demo.options.json --out ./demo/md",
    "demo:html": "typedoc --options ./demo.options.json --plugin typedoc-plugin-mdn-links --out ./demo/html"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoinColony/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoinColony/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/JoinColony/typedoc-plugin-markdown/tree/master/packages/typedoc-plugin-markdown",
  "keywords": [
    "markdown",
    "typescript",
    "documentation",
    "api",
    "typedocplugin",
    "typedoc"
  ],
  "peerDependencies": {
    "typedoc": ">=0.23.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "gitHead": "9faf6bf8ca03f2c5069febe8f74b0a16852c555f",
  "publishConfig": {
    "access": "public"
  }
}
