{
  "name": "typedoc-plugin-markdown",
  "version": "3.12.1",
  "description": "A plugin for TypeDoc that enables TypeScript API documentation to be generated in Markdown.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn run lint && yarn run build && && yarn run test",
    "build": "rm -rf dist && make-dir ./dist/resources && yarn run precompile && tsc",
    "test": "jest --colors --runInBand --updateSnapshot -t \"Comments:\"",
    "build-and-test": "yarn run build && yarn run test",
    "precompile": "handlebars ./src/resources/templates -c handlebars/runtime -f ./dist/resources/templates.js && handlebars ./src/resources/partials -c handlebars/runtime -p -f ./dist/resources/partials.js && node ./build/helpers.js",
    "demos": "yarn run build && yarn run demo:md && yarn run demo:html",
    "demo:md": "typedoc --plugin typedoc-plugin-markdown --options ../../test/typedoc-options.json --out ./demo/md",
    "demo:html": "typedoc --plugin none --options ../../test/typedoc-options.json --out ./demo/html"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-markdown/tree/master/packages/typedoc-plugin-markdown",
  "keywords": [
    "markdown",
    "typescript",
    "documentation",
    "api",
    "typedocplugin",
    "typedoc"
  ],
  "peerDependencies": {
    "typedoc": ">=0.22.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}
